{
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": ["@typescript-eslint", "prettier"],
	"extends": ["plugin:@typescript-eslint/recommended", "prettier"],
	"env": {
		"node": true
	},
	"rules": {
		"@typescript-eslint/no-non-null-assertion": "off",
		"@typescript-eslint/no-var-requires": "off",
		"prefer-const": "off"
	}
}
